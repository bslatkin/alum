{
  "name": "Alum",
  "version": "0.1",
  "description": "Window and tabs manager.",
  "background_page": "background.html",
  "options_page": "gridpicker.html",
  "browser_action": {
    "default_title": "Alum",
    "default_icon": "alum.jpg"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": [
      "tabs",
      "http://*/*",
      "https://*/*"
      ]
}