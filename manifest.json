{
  "name": "Alum: Window and tabs manager",
  "version": "0.1",
  "description": "Organize your screen into work zones and quickly switch between them.",
  "background_page": "background.html",
  "options_page": "gridpicker.html",
  "content_scripts": [
    {
      "matches": [
          "http://*/*",
          "https://*/*"
          ],
      "js": [
          "content.js"
          ]
    }
  ],
  "permissions": [
      "tabs",
      "http://*/*",
      "https://*/*"
      ]
}
